   "source": [
    "#Create x and y data and plot a figure\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(0, 10, 100)\n",
    "y = np.sin(x)   \n",
    "plt.plot(x, y)\n",
    "plt.xlabel('x-axis')\n",
    "plt.ylabel('y-axis')\n",
    "plt.title('Sine Wave')\n",
    "plt.grid()\n",
    "plt.savefig('output/sine_wave.png')\n",
    "plt.show()"
   ]
  }
